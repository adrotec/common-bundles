<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Adrotec\Common\CoreBundle\Entity\Country" table="_core_countries">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="code" type="string" column="code"/>
    <field name="name" type="string" column="name"/>
    <field name="telephoneCountryCode" type="integer" column="tel_country_code" nullable="true"/>
    <field name="telephoneExitCode" type="integer" column="tel_exit_code" nullable="true"/>
    <field name="telephoneTrunkCode" type="integer" column="tel_trunk_code" nullable="true"/>
  </entity>
</doctrine-mapping>
