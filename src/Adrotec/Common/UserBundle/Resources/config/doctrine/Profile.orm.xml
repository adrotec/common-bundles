<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Adrotec\Common\UserBundle\Entity\Profile" table="_usr_profiles">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="firstName" type="string" column="first_name"/>
        <!-- <field name="middleName" type="string" column="middle_name" nullable="true"/> -->
        <field name="lastName" type="string" column="last_name"/>
        <!-- <field name="initials" type="string" column="initials" nullable="true"/> -->
        <field name="gender" type="string" column="gender" nullable="true"/>
        <!-- <field name="marital" type="string" column="marital" nullable="true"/> -->
        <field name="dateOfBirth" type="date" column="date_of_birth" nullable="true"/>
        <!-- <field name="nationalIdentity" type="string" column="national_identity" nullable="true"/> -->
        <field name="url" type="string" column="url" nullable="true"/>
        <!-- <field name="titleName" type="string" column="title_name" nullable="true"/> -->
        <field name="bio" type="text" column="bio" nullable="true"/>

        
        <many-to-one field="photo" target-entity="Adrotec\Common\FSBundle\Entity\File">
            <join-column name="photo_id"></join-column>
        </many-to-one>       
        
    </entity>
</doctrine-mapping>